<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>@ViewBag.Title</title>
    <script src="@Url.Content("~/Scripts/jquery-1.4.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/FTWLabs.SecureMail.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/MicrosoftAjax.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/MicrosoftMvcAjax.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.lightbox.js")" type="text/javascript"></script>

    <link href="@Url.Content("~/content/oasisv2.css")" rel="Stylesheet" type="text/css" />
    <link href="@Url.Content("~/content/lightbox.css")" rel="Stylesheet" type="text/css" />
    @RenderSection("head", false)
    <style type="text/css">
        
        .top  
        {
            display: none; 
        }
        
        #ContentCallout 
        {
            display: none;
        }
        
        #Content
        {
            background-image: none;
            background-color: #ffffff;
        }
        
        #BottomRight
        {
            display: none;
        }
        
        #BottomLeft
        {
            width: 930px;
            background-color: #ffffff;
        }
        
        .navigation
        {
            display:none;
        }
        
        .bottom
        {
            padding: 20px;
        }
        
        .sub
        {
            display: none;            
        }
        
        #Footer
        {
            display: none;
        }
        
    </style>
    <script type="text/javascript">

        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-20481379-1']);
        _gaq.push(['_trackPageview']);

        (function () {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);


        })();

        $(document).ready(function () {

            $(".lightbox").lightbox({
                fitToScreen: true,
                imageClickClose: false
            });

        });
    </script> 

</head>
<body>
    <div class="outerWrapper">
        <div class="innerWrapper">
            <div id="Header">
                <div class="ir">
                    <h1>
                        <a href="@Url.Content("~/")"><em></em>Oasis Beach & Tennis Club</a></h1>
                </div>
                <div class="navigation">
                    <ul>
                        <li><a href="@Url.Content("~/")">home</a></li>
                        <li><a href="@Url.Content("~/about-us/")">about</a>
                            <div>
                                <ul>
                                    <li><a href="@Url.Content("~/staff/")">staff</a></li>
                                    <li><a href="@Url.Content("~/construction/")">construction updates</a></li>
                                    <li><a class="last" href="http://www.tennis.oasisbeachandtennisclub.com/pages/ecalendar/index.cfm?clubid=120630" target="_blank">calendar</a></li>
                                </ul>
                            </div>
                        </li>
                        <li><a href="@Url.Content("~/gallery/")">gallery</a></li>
                        <li><a href="@Url.Content("~/juniors/juniors/")">juniors</a>
                            <div>
                                <ul>
                                    <li><a href="@Url.Content("~/juniors/academy-program/")">academy</a></li>
                                    <li><a href="@Url.Content("~/juniors/spring-break-camps/")">spring break camps</a></li>
                                    <li><a href="@Url.Content("~/juniors/schooling-program/")">home school high performance</a></li>
                                    <li><a href="@Url.Content("~/juniors/travel-program/")">travel team</a></li>
                                    <li><a class="last" href="@Url.Content("~/juniors/quickstart-program/")">home school quickstart</a></li>
                                </ul>
                            </div>
                        </li>
                        <li><a href="@Url.Content("~/adults/adults/")">adults</a>
                            <div>
                                <ul>
                                    <li><a href="@Url.Content("~/adults/tournaments-and-mixers/")">tournaments & mixers</a></li>
                                    <li><a href="@Url.Content("~/adults/teams/")">teams</a></li>
                                    <li><a href="@Url.Content("~/adults/clinics/")">clinics</a></li>
                                    <li><a href="@Url.Content("~/adults/leagues/")">leagues</a></li>
                                    <li><a class="last" href="@Url.Content("~/adults/lessons/")">lessons</a></li>
                                </ul>
                            </div>
                        </li>
                        <li><a href="@Url.Content("~/membership/")">membership</a>
                            <div>
                                <ul>
                                    <li><a href="@Url.Content("~/membership/pool/")">pool</a></li>
                                    <li><a class="last" href="@Url.Content("~/policy/")">policies</a></li>
                                </ul>
                            </div>
                        </li>
                        <li><a href="@Url.Content("~/blog/")">blog</a></li>
                        <li><a href="@Url.Content("~/contact/")">contact</a></li>
                    </ul>
                </div>
            </div>
            <!-- Home page specific -->
            <div id="Content">
                <div class="top">
                    <div class="left">
                        <h2>
                            Located on a secluded, tree-studded property in Rockwall County, this brand new
                            country club offers <span>enjoyment for the whole family.</span></h2>
                    </div>
                    <div class="right">
                        <span>Get an Oasis <b>Membership Application</b></span> <a href="#" class="ir download">
                            <em></em>Download</a>
                    </div>
                </div>
                <div id="ContentCallout">
                    <div class="superWide">
                        <div class="imageWide">
                            <div id="ImageRoller">
                                <img src="/content/images/@(IsSectionDefined("headerimage") ? RenderSection("headerimage").ToString() : "Index01.jpg" )" alt="Oasis Beach & Tennis Club" />
                                <!--
                                <img src="Index02.jpg" style="display: none;" alt="Home1.jpg" />
                                <img src="/content/images/index03.jpg" style="display: none;" alt="Home1.jpg" />
                                -->
                            </div>
                            <div class="normalWide">
                                @if (IsSectionDefined("calloutblock"))
                                {
                                    @RenderSection("calloutblock")
                                }
                            </div>
                        </div>
                    </div>
                </div>
                <div class="bottom">
                        @RenderBody()
                </div>
                <div class="sub">
                    <div class="third1">
                        <ul>
                            <li>Clinics</li>
                            <li>Leagues</li>
                            <li>Juniors</li>
                        </ul>
                        <a href="@Url.Content("~/adults/")" class="ir learnmore"><em></em>Learn More</a>
                    </div>
                    <div class="third2">
                        <div>
                            <span>100&deg; or 30&deg; Outside</span> <span>Perfect 70&deg; Inside</span>
                        </div>
                    </div>
                    <div class="third3">
                        <span>5757 State Hwy 205</span> <span>Rockwall, TX 75032</span> <span class="phone">
                            (972) 772 7768</span> <a href="http://maps.google.com/maps?q=5757+State+Hwy+205+Rockwall,+TX+75032&ie=UTF8&hl=en&hq=&hnear=5757+Texas+205,+Rockwall,+Texas+75032&ll=32.831063,-96.362629&spn=0.038801,0.087376&z=14" class="ir map"><em></em>Map</a>
                    </div>
                </div>
            </div>
            <div id="Footer">
                <span>Copyright &copy; 2011 Oasis Beach & Tennis Club&nbsp;|&nbsp;
                    <a href="/policies/delivery-methods-and-timing/">Delivery Methods & Timing</a>&nbsp;|&nbsp;
                    <a href="/policies/return-refund-and-cancellation-policy/">Refunds Policy</a>&nbsp;|&nbsp;
                    <a href="/policies/privacy-policy/">Privacy Policy</a></span>
            </div>
        </div>
    </div>
</body>
</html>
